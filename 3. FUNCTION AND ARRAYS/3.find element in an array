import java.io.*;
import java.util.*;

public class Main{

public static void main(String[] args) throws Exception {
    // write your code here
    Scanner scn = new Scanner (System.in);

    int n= scn.nextInt();
    int arr[]= new int[n];

    for(int idx = 0; idx < arr.length; idx++){
    arr[idx]=scn.nextInt();

  }
    int d= scn.nextInt();
    

   int index = -1;
    for (int idx=0; idx<arr.length;idx++){
      if (arr[idx]==d){
        index=idx;
        break;
         }   








    }
    System.out.print(index);


 }

}






import java.io.*;
import java.util.*;

public class Main{

public static void main(String[] args){
Scanner scn = new Scanner (System.in);
int n = scn.nextInt();

int arr[] = new int[n];
for (int idx=0; idx<n; idx++){
  arr[idx]= scn.nextInt();

}

int d = scn.nextInt();


for (int idx= 0; idx < n; idx++){
if(arr[idx]==d){
    System.out.println(idx);
    return;
}


}
System.out.println("-1");












    
 }

}






import java.io.*;
import java.util.*;

public class Main{

public static void main(String[] args) {
    
    Scanner scn = new Scanner (System.in);
    int n = scn.nextInt();
    int arr[]= new int[n];
    for (int idx =0; idx<n; idx++){
         arr[idx]  = scn.nextInt();
         }
         int d = scn.nextInt();
         int ans =Arrayidx(arr,d);
         System.out.println(ans);


}
public static  int Arrayidx(int arr[], int val){
    for(int idx=0; idx<arr.length; idx++){
        if(arr[idx]==val){
            return idx;
        }

}
return -1;






}


}